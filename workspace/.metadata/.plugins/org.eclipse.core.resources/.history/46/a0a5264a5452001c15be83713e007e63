package com.koreait.ex01.java.quiz02;

import org.springframework.context.annotation.Bean;
import org.springframework.context.annotation.Configuration;

@Configuration
public class BeanConfig {
	@Bean
	public Gun gun() {
		Gun gun = new Gun();
		gun.setModel("k2");
		gun.setBullet(15);
		return gun;
	}
	

}
