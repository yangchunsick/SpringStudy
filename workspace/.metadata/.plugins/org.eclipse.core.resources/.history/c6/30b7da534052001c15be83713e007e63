package com.koreait.ex01.java.bean01;

public class Singer {
	
	private String name;
	private Song song;
	
	public Singer() {
		
	}
	
	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public Song getSong() {
		return song;
	}

	public void setSong(Song song) {
		this.song = song;
	}
}
