package com.koreait.ex01.bean01;

public class Calculator {

	// field
	
	// constructor
	public Calculator() {
		
	}
	
	// method
	public void add(int a, int b) {
		System.out.println(a + "+" + b + "=" + (a+b));
	}
	public void add(int a, int b) {
		System.out.println(a + "-" + b + "=" + (a-b));
	}
}
